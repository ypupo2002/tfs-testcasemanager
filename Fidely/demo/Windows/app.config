<?xml version="1.0"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
  </startup>
  <system.diagnostics>
    <trace autoflush="true"/>
    <sources>
      <source name="Fidely.Framework.Logger" switchName="Fidely.Framework.Logger" switchType="System.Diagnostics.SourceSwitch">
        <listeners>
          <add name="inmemory"/>
        </listeners>
      </source>
    </sources>
    <switches>
      <add name="Fidely.Framework.Logger" value="All"/>
    </switches>
    <sharedListeners>
      <add name="inmemory" type="Fidely.Demo.Windows.Model.InMemoryTraceListener, Fidely.Demo.Windows"/>
    </sharedListeners>
  </system.diagnostics>
</configuration>
